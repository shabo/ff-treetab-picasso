# Local publish configuration for AMO (not committed when copied to env.yaml)
# Copy this file to env.yaml and fill in credentials.
#
# You can create env.yaml automatically:
#   make env

# Required for AMO signing/publishing.
AMO_JWT_ISSUER: ""
AMO_JWT_SECRET: ""

# Optional: listed (default) or unlisted
AMO_CHANNEL: "listed"

# Optional:
# 0 => build can skip publish when credentials are missing
# 1 => fail build/publish if credentials are missing
AMO_REQUIRE_PUBLISH: "0"
